  <code>global.checkpoint_id = noone;
global.checkpoint_r = 0;
global.checkpoint_x = 0;
global.checkpoint_y = 0;
globalvar day;
globalvar time;
cursor_sprite = spr_crossHair;

ini_open("playerData.ini");

if (ini_key_exists("Player", "day")) {
    day = ini_read_real("Player", "day", 0);
} else {
    day = 0;
}
if (ini_key_exists("Player", "hours")) {
    time = ini_read_real("Player", "hours", 12);
} else {
    time = 0
}
if (ini_key_exists("Player", "health")) {
    time = ini_read_real("Player", "hours", 100);
} else {
    time = 100
}
if (ini_key_exists("Player", "hours")) {
    global.maxStamina = ini_read_real("Player", "stamina", 100);
} else {
    global.maxHealth = 100
}
if (ini_key_exists("Player", "name")) {
    name = ini_read_string("Player", "name", "");
} else {
    name = "";
}

if(ini_read_string("Player","isDead","yes") = "yes")
{    
    // Player data initialize
    ini_write_string("Player","isDead","no");
    ini_write_real("Player","health",100);
    ini_write_real("Player","stamina",100);
    ini_write_real("Player","hours",12);
    
}    

ini_close();

room_goto(rm_menu);</code>
