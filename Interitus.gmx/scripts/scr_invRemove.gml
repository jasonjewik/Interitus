for (var i = 1; i < maxInvSlots; i++) {
    if (global.inventory[i, 0] = global.keyBinding[1, 0] && global.inventory[i, 0] != "---") {
        global.inventory[i, 0] = "";
    } else if (global.inventory[i, 0] = global.keyBinding[2, 0] && global.inventory[i, 0] != "---") {
        global.inventory[i, 0] = "";
    }  
}
