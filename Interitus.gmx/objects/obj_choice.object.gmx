            <string>///Draw the string and the box
//Setting up some stuffs
draw_self();
texture_set_interpolation(false);
draw_set_color(c_white);
draw_set_font(fnt_text);

//Past the width, insert line break
if (string_width(str) &gt; width - padding - padding) {
    message = string_delete(message, last_space, 1);
    message = string_insert("#", message, last_space);
    ds_list_add(choice, last_space + 1);
}

//End of message
if (count &lt; string_length(message) + 2) {
    if (string_char_at(message, count) == " ") {
        last_space = count;
    }
    count += 0.25;
}

//Move up a line
if (string_height(str) &gt; height - padding) {
    line++;
}

//Grab String
str = string_copy(message, ds_list_find_value(choice, line), count - ds_list_find_value(choice, line));

//Draw Text
draw_text(room_width/2 + padding, room_height/2 + padding, str);
</string>
